{
  "name": "backstage",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "aws:dev": "export AWS_PROFILE=content-dev && export AWS_CONFIG_FILE=.aws/config && aws sso login",
    "aws:refresh": "./.aws/refresh-credentials.sh",
    "build:server": "tsc --project tsconfig.server.json",
    "build:next": "next build",
    "build:analyse": "ANALYSE_BUNDLE=true npm run build:next",
    "build": "npm run build:next && npm run build:server",
    "build:e2e": "dotenv -e .env.test -- cross-env NODE_ENV=production npm run build",
    "start": "cross-env NODE_ENV=production node dist/server.js",
    "start:e2e": "dotenv -e .env.test -- cross-env NODE_ENV=production node dist/server.js",
    "start:dev": "cross-env NODE_ENV=development tsx --tsconfig tsconfig.server.json src/server.ts",
    "lint": "next lint",
    "test:unit": "jest --ci --runInBand --bail",
    "test:unit:watch": "jest --watch",
    "test:e2e": "dotenv -e .env.test -- playwright test",
    "test:e2e:ui": "dotenv -e .env.test -- playwright test --ui",
    "prettier": "prettier --write .",
    "prepare": "husky install",
    "validate": "npm run lint && npm run test:unit && npm run build:e2e && npm run test:e2e",
    "localstack": "npm run localstack:init && sh localstack/utils/scripts/start.sh",
    "localstack:terraform": "npm run localstack:init && sh localstack/utils/scripts/terraform.sh",
    "localstack:teardown": "npm run localstack:init && sh localstack/utils/scripts/teardown.sh",
    "localstack:init": "git submodule update --init"
  },
  "dependencies": {
    "@aws-sdk/client-cognito-identity-provider": "^3.292.0",
    "@aws-sdk/client-s3": "^3.292.0",
    "@aws-sdk/client-sns": "^3.468.0",
    "@aws-sdk/credential-providers": "^3.292.0",
    "@aws-sdk/s3-request-presigner": "^3.294.0",
    "@aws-sdk/types": "^3.338.0",
    "@axe-core/playwright": "^4.6.1",
    "@common/logger": "^2.0.9",
    "@common/prometheus": "^2.3.8",
    "@emotion/react": "^11.9.3",
    "@emotion/styled": "^11.9.3",
    "@faker-js/faker": "^8.4.1",
    "@hookform/resolvers": "^2.9.7",
    "@mui/icons-material": "^5.8.4",
    "@mui/material": "^5.8.7",
    "@mui/system": "^5.9.2",
    "@mui/utils": "^5.10.9",
    "@next/env": "^13.5.4",
    "@playwright/test": "^1.34.3",
    "@segment/analytics-next": "^1.70.0",
    "@sentry/nextjs": "^8.10.0",
    "@tanstack/react-query": "^4.22.0",
    "@tanstack/react-query-devtools": "^4.22.0",
    "@trpc/client": "^10.8.1",
    "@trpc/next": "^10.8.1",
    "@trpc/react-query": "^10.8.1",
    "@trpc/server": "^10.8.1",
    "@types/js-cookie": "^3.0.3",
    "axios": "^1.7.2",
    "braces": "3.0.3",
    "copy-webpack-plugin": "^12.0.2",
    "dayjs": "^1.11.7",
    "express": "^4.18.2",
    "framer-motion": "^7.2.0",
    "isomorphic-fetch": "^3.0.0",
    "js-cookie": "^3.0.5",
    "lodash.isequal": "^4.5.0",
    "mediainfo.js": "^0.2.0",
    "micromatch": "4.0.6",
    "msw": "^1.3.2",
    "next": "^14.2.3",
    "next-auth": "^4.17.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.33.1",
    "recharts": "^2.12.3",
    "snowflake-sdk": "^1.10.1",
    "ts-node": "^10.9.1",
    "ua-parser-js": "^1.0.36",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@aws-sdk/client-sqs": "^3.470.0",
    "@hotjar/browser": "^1.0.7",
    "@next/bundle-analyzer": "^14.1.4",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/async-retry": "^1.4.8",
    "@types/express": "^4.17.16",
    "@types/lodash.isequal": "^4.5.6",
    "@types/node": "17.0.31",
    "@types/react": "^18.3.2",
    "@types/react-dom": "^18.3.0",
    "@types/snowflake-sdk": "^1.6.20",
    "@types/ua-parser-js": "^0.7.37",
    "@typescript-eslint/eslint-plugin": "^5.30.6",
    "async-retry": "^1.3.3",
    "axios-mock-adapter": "^1.22.0",
    "blob-polyfill": "^7.0.20220408",
    "cross-env": "^7.0.3",
    "dotenv": "^16.4.5",
    "dotenv-cli": "^7.4.1",
    "eslint": "^8.20.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-next": "^14.2.3",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-react": "^7.30.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.1",
    "jest": "^29.3.1",
    "jest-environment-jsdom": "^29.3.1",
    "jest-watch-typeahead": "^2.0.0",
    "lint-staged": "^12.4.1",
    "next-router-mock": "^0.8.0",
    "prettier": "2.6.2",
    "tsx": "^4.13.1",
    "typescript": "^4.0.0"
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown",
    "*.ts": "eslint"
  },
  "msw": {
    "workerDirectory": "public"
  }
}
